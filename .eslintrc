{
  "extends": ["react-app", "prettier"],
  "plugins": ["react","prettier"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "printWidth": 80,
        "tabWidth": 2,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "arrowParens": "avoid",
        "endOfLine": "auto"
      }

    ]
  }
}